        ##############################
        # Chemistry related variables
        ##############################

do_full_chem: T
do_qqjk_inchem: F
do_synoz: F
chem_opt: 2
dehyd_opt: 0
do_chem_grp: T
synoz_threshold: 150.0d-9
chem_cycle: 1.0d0
do_smv_reord: F
do_wetchem: F
loss_opt: 0
do_AerDust_Calc: T
AerDust_Effect_opt: 0

        ###########################################################
        # GHG and ODS surface source gases
        # forc_bc_opt: 1=Time slice, 2=Time-dependent, 3=Calculated
        ###########################################################

forc_bc_opt: 2
forc_bc_years: 69
forc_bc_start_num: 1950
forc_bc_kmin: 1
forc_bc_kmax: 2
